              <stringProp name="script">// Chuẩn set dữ liệu Ngày đặt, ngày kết thúc
public String formatDate(Date dateTime) {
	Calendar cal = Calendar.getInstance();
	cal.setTime(dateTime);
	// dd/MM/yyyy
	StringBuffer buff = new StringBuffer();
	buff.append(cal.get(Calendar.DAY_OF_MONTH) &lt; 10 ? &quot;0&quot; + cal.get(Calendar.DAY_OF_MONTH) : cal.get(Calendar.DAY_OF_MONTH)).append(&quot;/&quot;);
	buff.append((cal.get(Calendar.MONTH) + 1) &lt; 10 ? &quot;0&quot; + (cal.get(Calendar.MONTH) + 1) : (cal.get(Calendar.MONTH) + 1)).append(&quot;/&quot;);
	buff.append(cal.get(Calendar.YEAR));
}

public Date adjustDate(Date dateTime, int numberOfDay) {
	Calendar cal = Calendar.getInstance();
	cal.setTime(dateTime);
	cal.add(Calendar.DAY_OF_MONTH, numberOfDay);
	return cal.getTime();
	
}


